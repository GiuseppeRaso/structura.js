{
  "name": "structura-js",
  "private": false,
  "version": "0.1.0",
  "main": "./dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --project tsconfig.build.json && vite build && tsc --project tsconfig.build.declarations.json",
    "preview": "vite preview",
    "test": "vitest --coverage",
    "test:prod": "vitest run",
    "benchmark": "node --experimental-specifier-resolution=node --loader ts-node/esm ./src/benchmark",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs"
  },
  "devDependencies": {
    "@types/node": "^17.0.42",
    "benny": "^3.7.1",
    "c8": "^7.11.3",
    "immer": "^9.0.15",
    "immutable": "^4.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vitepress": "^1.0.0-alpha.13",
    "vitest": "^0.14.2",
    "vue": "^3.2.38"
  }
}
